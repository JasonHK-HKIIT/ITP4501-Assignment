<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
    <item>
        <shape>
            <solid android:color="?attr/colorSurface" />
            <corners
                android:bottomLeftRadius="?attr/dialogCornerRadius"
                android:bottomRightRadius="?attr/dialogCornerRadius"
                android:topLeftRadius="?attr/dialogCornerRadius"
                android:topRightRadius="?attr/dialogCornerRadius" />
        </shape>
    </item>
    <item>
        <shape>
            <solid
                android:color="@color/m3_popupmenu_overlay_color"
                tools:ignore="PrivateResource" />
            <corners
                android:bottomLeftRadius="?attr/dialogCornerRadius"
                android:bottomRightRadius="?attr/dialogCornerRadius"
                android:topLeftRadius="?attr/dialogCornerRadius"
                android:topRightRadius="?attr/dialogCornerRadius" />
        </shape>
    </item>
</layer-list>
